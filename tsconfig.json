{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "jsx": "react-jsx",
  "moduleResolution": "Node",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "strict": false,
    "types": ["vitest/globals"],
    "composite": true
  },
  "include": ["src"],
  "exclude": ["node_modules", "dist"]
}
