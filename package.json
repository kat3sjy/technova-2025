{
  "name": "technova-2025",
  "version": "0.1.0",
  "private": true,
  "description": "Networking app frontend for underrepresented groups in male-dominated fields (gaming, tech conventions, sports).",
  "type": "module",
  "scripts": {
    "predev": "kill-port 5173 || true && kill-port 3000 || true",
    "dev": "concurrently -k -n BACKEND,WEB -c blue,green \"npm:dev:backend\" \"npm:dev:web\"",
    "dev:backend": "npm --prefix backend run dev",
    "dev:web": "vite"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.26.2",
    "zustand": "^4.5.2",
    "socket.io-client": "^4.7.5",
    "nanoid": "^5.0.7"
  },
  "devDependencies": {
    "@babel/runtime": "^7.25.0",
    "concurrently": "^8.2.2",
    "kill-port": "^2.0.1",
    "@vitejs/plugin-react": "^4.2.0",
    "vite": "^5.4.0",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@types/node": "^22.5.5",
    "typescript": "^5.5.4"
  }
}
