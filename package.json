{
  "name": "technova-2025",
  "version": "0.1.0",
  "private": true,
  "description": "Networking app frontend for underrepresented groups in male-dominated fields (gaming, tech conventions, sports).",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"vite\" \"npm:server\"",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "test:ui": "vitest",
    "server": "tsx server/index.ts",
    "build:server": "tsc -p server/tsconfig.json"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.26.2",
    "zustand": "^4.5.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.6",
    "@testing-library/react": "^16.0.1",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.5.5",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "concurrently": "^8.2.2",
    "eslint": "^9.10.0",
    "eslint-plugin-react": "^7.35.0",
    "jsdom": "^24.1.1",
    "tsx": "^4.16.2",
    "typescript": "^5.5.4",
    "vite": "^5.4.1",
    "vitest": "^2.1.1"
  }
}
